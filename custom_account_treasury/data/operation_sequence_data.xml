<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Secuencia para Operaciones de Entrada -->
    <record id="sequence_payment_operation_entry" model="ir.sequence">
        <field name="name">Operación de Entrada</field>
        <field name="code">account.payment.operation.entry</field>
        <field name="prefix">ENT/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
        <field name="use_date_range">True</field>
    </record>

    <!-- Secuencia para Operaciones de Salida -->
    <record id="sequence_payment_operation_exit" model="ir.sequence">
        <field name="name">Operación de Salida</field>
        <field name="code">account.payment.operation.exit</field>
        <field name="prefix">SAL/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
        <field name="use_date_range">True</field>
    </record>
</odoo>