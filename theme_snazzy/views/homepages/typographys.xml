<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="theme_snazzy_typographys_page_template" model="ir.ui.view">
        <field name="name">Theme Snazzy Typographys</field>
        <field name="type">qweb</field>
        <field name="key">website.theme_snazzy_typographys_page_template</field>
        <field name="arch" type="xml">
            <t name="Homepage One E-commerce" t-name="theme_snazzy.theme_snazzy_typographys_page_template">
                <t t-call="website.layout">
                    <div id="wrap" class="oe_structure oe_empty">

                        <section class="ml64 mt32 mb32">

                            <h1 class="mb24">h1 - Theme Snazzy</h1>

                            <h2 class="mb24">h2 - Theme Snazzy</h2>

                            <h3 class="mb24">h3 - Theme Snazzy</h3>

                            <h4 class="mb24">h4 - Theme Snazzy</h4>

                            <h5 class="mb24">h5 - Theme Snazzy</h5>

                            <h6 class="mb24">h6 - Theme Snazzy</h6>

                            <p class="mb24">p - Theme Snazzy</p>

                            <div  class="mb24">
                                <small>small - Theme Snazzy</small>
                            </div>

                            <div class="mb24">div - Theme Snazzy</div>

                            <div class="mb-24">
                                <span>span - Theme Snazzy</span>
                            </div>

                        </section>

                        <div class="oe_structure oe_empty"></div>

                    </div>
                </t>
            </t>
        </field>
    </record>

    <record id="theme_snazzy_typographys_page_template_menu" model="website.page">
        <field name="website_published">True</field>
        <field name="url">/typography</field>
        <field name="view_id" ref="theme_snazzy_typographys_page_template" />
    </record>

</odoo>






