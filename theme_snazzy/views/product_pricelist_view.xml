<odoo>
    <record id="product_pricelist_view_deal" model="ir.ui.view">
        <field name="name">product.pricelist.form</field>
        <field name="model">product.pricelist</field>
        <field name="inherit_id" ref="product.product_pricelist_view"></field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='selectable']" position="after">
                <field name="show_before_days"/>
            </xpath>
            <xpath expr="//field[@name='website_id']" position="after">
                <field name="auto_assign"/>
            </xpath>
        </field>
    </record>
</odoo>