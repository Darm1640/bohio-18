<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="snippet_options_inherited" inherit_id="website.snippet_options">
		<!-- Headers -->
		<xpath expr="//we-select[@data-variable='header-template']" position="inside">
			<we-button title="Snazzy Header One" data-name="snazzy_header_one_opt" data-customize-website-views="theme_snazzy.theme_snazzy_header1" data-customize-website-variable="'snazzy-header-1'" data-img="/theme_snazzy/static/description/header1.png"/>
			<we-button title="Snazzy Header Two" data-name="snazzy_header_two_opt" data-customize-website-views="theme_snazzy.theme_snazzy_header2" data-customize-website-variable="'snazzy-header-2'" data-img="/theme_snazzy/static/description/header2.png"/>
			<we-button title="Snazzy Header Three" data-name="snazzy_header_three_opt" data-customize-website-views="theme_snazzy.theme_snazzy_header3" data-customize-website-variable="'snazzy-header-3'" data-img="/theme_snazzy/static/description/header3.png"/>
			<we-button title="Snazzy Header Four" data-name="snazzy_header_four_opt" data-customize-website-views="theme_snazzy.theme_snazzy_header4" data-customize-website-variable="'snazzy-header-4'" data-img="/theme_snazzy/static/description/header4.png"/>
			<we-button title="Snazzy Header Five" data-name="snazzy_header_five_opt" data-customize-website-views="theme_snazzy.theme_snazzy_header5" data-customize-website-variable="'snazzy-header-5'" data-img="/theme_snazzy/static/description/header5.png"/>
			<we-button title="Snazzy Header Six" data-name="snazzy_header_six_opt" data-customize-website-views="theme_snazzy.theme_snazzy_header6" data-customize-website-variable="'snazzy-header-6'" data-img="/theme_snazzy/static/description/header6.png"/>
		</xpath>

		<xpath expr="//div[@data-js='HeaderNavbar']" position="attributes">
			<attribute name="data-selector">#wrapwrap > header</attribute>
		</xpath>

		<xpath expr="//div[@data-selector='#wrapwrap > header nav.navbar .o_header_language_selector']" position="attributes">
			<attribute name="data-selector">#wrapwrap > header</attribute>
		</xpath>

		<xpath expr="//div[@data-js='HeaderNavbar']" position="inside">
			<we-checkbox string="Show Pricelist" data-customize-website-views="theme_snazzy.snazzy_header_pricelist" data-reload="/"/>
		</xpath>

		<xpath expr="//div[@data-js='HeaderNavbar']" position="after">
			<div data-selector="#wrapwrap > header .snazzy_header" data-no-check="true">
				<we-row string="Show Cart Sidebar">
					<we-checkbox data-customize-website-views="theme_snazzy.show_snazzy_cart_sidebar" data-reload="/"/>
				</we-row>
			</div>
		</xpath>

		<xpath expr="//we-select[@data-variable='header-template']" position="after">
			<we-row string="Extra Header Colors" data-dependencies="snazzy_header_one_opt">
				<!-- HEADER CUSTOM COLORS -->
				<we-colorpicker title="Top BG Color" data-customize-website-color="" data-color-type="theme" data-color="header-custom-top-background" data-dependencies="snazzy_header_one_opt,snazzy_header_two_opt,snazzy_header_five_opt" data-name="header_custom_top_background"/>
			</we-row>
		</xpath>

		<!-- Footers -->
		<xpath expr="//we-select[@data-variable='footer-template']" position="inside">
			<we-button title="Snazzy Footer 1" data-name="snazzy_footer_one_opt" data-customize-website-views="theme_snazzy.theme_common_footer1" data-customize-website-variable="'snazzy-footer-1'" data-img="/theme_snazzy/static/description/footer1.png"/>
			<we-button title="Snazzy Footer 2" data-name="snazzy_footer_two_opt" data-customize-website-views="theme_snazzy.theme_common_footer2" data-customize-website-variable="'snazzy-footer-2'" data-img="/theme_snazzy/static/description/footer2.png"/>
			<we-button title="Snazzy Footer 3" data-name="snazzy_footer_three_opt" data-customize-website-views="theme_snazzy.theme_common_footer3" data-customize-website-variable="'snazzy-footer-3'" data-img="/theme_snazzy/static/description/footer3.png"/>
			<we-button title="Snazzy Footer 4" data-name="snazzy_footer_four_opt" data-customize-website-views="theme_snazzy.theme_common_footer4" data-customize-website-variable="'snazzy-footer-4'" data-img="/theme_snazzy/static/description/footer4.png"/>
			<we-button title="Snazzy Footer 5" data-name="snazzy_footer_five_opt" data-customize-website-views="theme_snazzy.theme_common_footer5" data-customize-website-variable="'snazzy-footer-5'" data-img="/theme_snazzy/static/description/footer5.png"/>
			<we-button title="Snazzy Footer 6" data-name="snazzy_footer_six_opt" data-customize-website-views="theme_snazzy.theme_common_footer6" data-customize-website-variable="'snazzy-footer-6'" data-img="/theme_snazzy/static/description/footer6.png"/>
			<we-button title="Snazzy Footer 7" data-name="snazzy_footer_seven_opt" data-customize-website-views="theme_snazzy.theme_common_footer7" data-customize-website-variable="'snazzy-footer-7'" data-img="/theme_snazzy/static/description/footer7.png"/>
			<we-button title="Snazzy Footer 8" data-name="snazzy_footer_eight_opt" data-customize-website-views="theme_snazzy.theme_common_footer8" data-customize-website-variable="'snazzy-footer-8'" data-img="/theme_snazzy/static/description/footer8.png"/>
			<we-button title="Snazzy Footer 9" data-name="snazzy_footer_nine_opt" data-customize-website-views="theme_snazzy.theme_common_footer9" data-customize-website-variable="'snazzy-footer-9'" data-img="/theme_snazzy/static/description/footer9.png"/>
		</xpath>
	</template>

