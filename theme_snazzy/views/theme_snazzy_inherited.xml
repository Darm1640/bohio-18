	<!-- Mega menus -->
	<template id="megamenu_snippets_inherited" inherit_id="website.snippet_options">
		<xpath expr="div[@data-js='MegaMenuLayout']//we-select" position="inside">
			<t t-set="_label">Snazzy Menu One</t>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_one_snippet" data-dependencies="!fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_one.jpg" t-out="_label"/>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_category_one_snippet" data-dependencies="fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_dynamic.jpg" t-out="_label"/>

			<t t-set="_label">Snazzy Menu Two</t>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_two_snippet" data-dependencies="!fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_two.jpg" t-out="_label"/>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_category_two_snippet" data-dependencies="fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_two.jpg" t-out="_label"/>

			<t t-set="_label">Snazzy Menu Three</t>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_three_snippet" data-dependencies="!fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_three.jpg" t-out="_label"/>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_category_three_snippet" data-dependencies="fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_three.jpg" t-out="_label"/>

			<t t-set="_label">Dynamic Mega Menu</t>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_dynamic_snippet" data-dependencies="!fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_dynamic.jpg" t-out="_label"/>

			<t t-set="_label">Snazzy Menu Four</t>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_four_snippet" data-dependencies="!fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_four.jpg" t-out="_label"/>

			<t t-set="_label">Snazzy Menu Five</t>
			<we-button t-att-data-select-label="_label" data-select-template="snazzy_theme_common.s_mega_menu_five_snippet" data-dependencies="!fetch_ecom_categories_opt" data-img="/theme_snazzy/static/src/img/mega_menu_five.jpg" t-out="_label"/>
		</xpath>
		<xpath expr="//div[@data-js='MegaMenuLayout']//we-row[@t-if='categories']" position="replace">
			<we-row t-if="categories" class="hasCategoriesToggle">
				<we-title class="o_long_title">eCommerce Categories</we-title>
				<div class="o_switch ms-4">
					<we-checkbox
						class="o_mega_menu"
						data-name="fetch_ecom_categories_opt"
						data-select-class="fetchEcomCategories"
						data-toggle-fetch-ecom-categories=""
						data-no-preview="true"
					/>
				</div>
				<we-button
					type="button"
					title="Reset Categories"
					data-refresh-mega-menu-template=""
					data-no-preview="true"
					class="ms-4"
				>
					<i class="fa fa-refresh"/>
				</we-button>
			</we-row>
		</xpath>
	</template>

	<!-- Heading tags style options -->
	<template id="heading_style_options" inherit_id="website.snippet_options">
		<xpath expr="//div[@data-js='OptionsTab']" position="inside">
			<we-title>Bizople Custom Settings</we-title>
			<we-row string="Product Background Color">
				<we-colorpicker string="" class="o_we_sublevel_1" data-select-style="true" data-customize-website-color="" data-color-type="theme" data-color="product-bgcolor" data-css-property="background-color" data-color-prefix="bg-" data-variable="product-bgcolor" />
			</we-row>
			<div>
				<we-checkbox string="Login Signup Popup option" data-customize-website-views="theme_snazzy.loginsignup_popup_bizt" data-reload="/"/>
				<we-checkbox string="Show Product Background Color" data-customize-website-views="theme_snazzy.product_background_color" data-reload="/"/>
				<we-checkbox string="Infinite Load" data-customize-website-views="theme_snazzy.snazzy_infinite_load" data-reload="/"/>
				<we-checkbox string="Lazy Load" data-customize-website-views="theme_snazzy.snazzy_lazy_load" data-reload="/"/>
				<we-checkbox string="Reorder" data-customize-website-views="theme_snazzy.snazzy_website_reorder" data-reload="/"/>
				<we-checkbox string="B2B Mode" data-customize-website-views="theme_snazzy.snazzy_b2b_mode" data-reload="/"/>
			</div>

		</xpath>

		<xpath expr="//we-checkbox[@data-customize-website-views='website_sale.filter_products_price']" position="before">
			<we-checkbox string="Brands" class="o_we_sublevel_1" data-customize-website-views="theme_snazzy.products_brands" data-dependencies="attributes_opt, attributes_opt_top" data-no-preview="true" data-reload="/"/>
			<we-checkbox string="Filter Products By Stock" class="o_we_sublevel_1" data-customize-website-views="theme_snazzy.snazzy_filter_instock_products" data-dependencies="attributes_opt, attributes_opt_top" data-no-preview="true" data-reload="/"/>
		</xpath>
	</template>

	<!-- Icon Selector -->
	<template id="icon_selector_inherit" inherit_id="web_editor.snippet_options">
		<xpath expr="//div[@data-js='ReplaceMedia']" position="attributes">
			<attribute name="data-selector">img, .media_iframe_video, span.fa, i.fa, .ri</attribute>
		</xpath>

		<xpath expr="//div[@data-js='FontawesomeTools']" position="attributes">
			<attribute name="data-selector">span.fa, i.fa, .ri</attribute>
		</xpath>

		<xpath expr="//div[@data-selector='span.fa, i.fa, img']" position="attributes">
			<attribute name="data-selector">span.fa, i.fa, img, .ri</attribute>
		</xpath>
	</template>
</odoo>